    "md5": "~2.0.0",
    "moment": "^2.18.1",
    "morgan": "~1.6.1",
    "mysql": "~2.9.0",
    "node-cron": "1.1.1",
    "node-jspdf": "git://github.com/calvinfroedge/node-jspdf.git",
    "node-rest-client": "2.0.1",
